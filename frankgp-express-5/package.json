{
  "name": "frankgp-nodejs",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "scripts": {
    "nodemon": "npx nodemon",
    "start": "node dist/main.js",
    "dev": "npx nodemon",
    "start:node": "ts-node src/main.ts",
    "start:dev": "ts-node-dev --respawn src/main.ts",
    "mono": "nodemon --exec ts-node ./mono/mono.ts",
    "build": "node scripts/clean-dist.js && tsc",
    "build2": "tsc",
    "seed": "ts-node src/seed/seeder.ts",
    "deploy": "cd ../frankgp-dist && git add . && git commit --amend --no-edit && git push --force"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.32",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/socket.io": "^3.0.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@nestjs/mapped-types": "*",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.0",
    "mysqldump": "^3.2.0",
    "nanoid": "^3.3.11",
    "node-cron": "^4.0.5",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "typeorm": "0.3.23"
  }
}
